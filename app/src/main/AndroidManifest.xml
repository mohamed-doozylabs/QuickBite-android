<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.griffsoft.tsadadelivery">

    <uses-permission android:name="android.permission.SEND_SMS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:theme="@style/AppTheme">
        <activity
            android:name=".TDTabBarActivity"
            android:label="@string/title_activity_tdtab_bar"></activity>
        <activity android:name=".AddNewAddressSearchViewActivity" android:parentActivityName=".LoginActivity" />
        <activity android:name=".TDActivity" />
        <activity
            android:name=".AppContainerActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".OrderActivity"
            android:label="Order"
            android:parentActivityName=".MainActivity" />
    </application>

</manifest>